{
  "name": "baseWebFramwork",
  "version": "0.0.0",
  "description": "todo",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "tslint-check-test": "tslint-config-prettier-check ./test/tslint.json",
    "prettier": "prettier --write",
    "tslint": "tslint -t stylish -p ./tsconfig.json  -c ./tslint.json --force --fix",
    "tslint-test": "tslint -t stylish -p ./test/tsconfig.json  -c ./test/tslint.json --force --fix",
    "validatemsg": "commitlint -e",
    "validatemsgpush": "commitlint -t $GIT_PUSH_TO -f $GIT_PUSH_FROM",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "Robert Kesterson <rkesters@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/config-conventional": "^8.1.0",
    "body-parser": "1.19.0",
    "commitlint": "8.1.0",
    "del": "5.0.0",
    "express": "4.17.1",
    "husky": "3.0.1",
    "i": "0.3.6",
    "jest": "24.8.0",
    "jsdom": "15.1.1",
    "lint-staged": "9.2.0",
    "minimatch": "3.0.4",
    "morgan": "1.9.1",
    "nightwatch": "1.7.13",
    "prettier": "1.18.2",
    "request": "2.88.0",
    "selenium-webdriver": "4.0.0-alpha.4",
    "sinon": "7.3.2",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-airbnb-base": "0.3.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "animate.css": "3.7.2",
    "backbone": "1.4.0",
    "backbone-routing": "0.2.0",
    "backbone-service-modals": "0.2.0",
    "backbone.marionette": "4.1.2",
    "backbone.radio": "2.0.0",
    "bootstrap": "4.3.1",
    "handlebars": "4.1.2",
    "jquery": "3.4.1",
    "lodash": "4.17.14",
    "popper.js": "^1.15.0",
    "pretty-diff": "0.7.1",
    "unibeautify": "^0.17.0",
    "unibeautify-cli": "^0.2.1",
    "yargs": "^13.3.0"
  },
  "lint-staged": {
    "./**/*.{less,json,css,md}": [
      "prettier --write --no-editorconfig",
      "git add"
    ],
    "*.{js}": [
      "prettier --write --no-editorconfig --trailing-comma es5",
      "git add"
    ],
    "./src/**/*.handlebars": [
      "prettier --write --no-editorconfig --parser vue",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "prettier --write --no-editorconfig",
      "tslint -t stylish -p ./tsconfig.json  -c ./tslint.json --fix",
      "prettier --write --no-editorconfig",
      "git add"
    ],
    "test/**/*.{ts,tsx}": [
      "tslint -t stylish -p ./test/tsconfig.json  -c ./test/tslint.json --fix",
      "prettier --write --no-editorconfig",
      "git add"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100
  },
  "unibeautify": {
    "TypeScript": {
      "beautifiers": [
        "Prettier"
      ]
    },
    "CSS": {
      "indent_char": " ",
      "indent_size": 2
    },
    "HTML": {
      "indent_char": " ",
      "indent_size": 2
    }
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "lint-staged",
      "pre-push": "npm test",
      "prepare-commit-msg": "bash ./commitMsg.sh",
      "commit-msg": "npm run -s validatemsg"
    }
  }
}
